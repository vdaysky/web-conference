<Page x:Class="WebConf.screens.RoomPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WebConf.screens"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RoomPage">

    <Grid>
        <TreeView x:Name="MessageTree" HorizontalAlignment="Left" Height="300" Margin="24,38,0,0" VerticalAlignment="Top" Width="584">
            <TreeViewItem x:Name="RoomMsgContainer" Header="RoomName"/>
        </TreeView>
        <TextBox x:Name="MessageInput" HorizontalAlignment="Left" Height="28" Margin="24,357,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="507"/>
        <Button Content="Send" HorizontalAlignment="Left" Margin="548,357,0,0" VerticalAlignment="Top" Width="60" Height="28" Click="Button_Click"/>

    </Grid>
</Page>
